set nocompatible
filetype off
set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()
Plugin 'gmarik/Vundle.vim'
call vundle#end()
filetype plugin indent on
set nu
let g:python_highlight_all=1
syntax on
set background=dark
"colorscheme solarized

set shiftwidth=4
set tabstop=4

set expandtab

set smartindent
set autoindent
